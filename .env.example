# Braintrust API credentials
BRAINTRUST_API_KEY=your-api-key-here

# Optional: Organization name (will be fetched from API if not provided)
# BRAINTRUST_ORG_NAME=your-org-name

# Optional: Default project
# BRAINTRUST_DEFAULT_PROJECT_NAME=my-project
# BRAINTRUST_DEFAULT_PROJECT_ID=proj-123

# Optional: Custom API endpoints (defaults shown)
# BRAINTRUST_APP_URL=https://www.braintrust.dev
# BRAINTRUST_API_URL=https://api.braintrust.dev

# OpenAI API key (for examples and testing)
OPENAI_API_KEY=your-openai-api-key-here
